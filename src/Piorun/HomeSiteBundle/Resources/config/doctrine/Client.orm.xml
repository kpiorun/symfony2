<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Piorun\HomeSiteBundle\Entity\Client" table="client">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="text" column="name" nullable="false"/>
    <field name="lastname" type="text" column="lastname" nullable="false"/>
    <field name="partnercodeid" type="bigint" column="partnercodeid" nullable="true"/>
    <field name="creationdate" type="datetime" column="creationdate" nullable="false"/>
  </entity>
</doctrine-mapping>
